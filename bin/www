const app = require("../server")
const PORT = process.env.PORT || 5000;

const onError = error => {
  console.log(error, "error")
}

const onListening = () => {
  const address = process.env.API_URL || `http://localhost:${PORT}`
  console.log(`Server is running on ${address}`)
}

app
  .listen(PORT)
  .on("error",onError)
  .on("listening",onListening)